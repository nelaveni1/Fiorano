<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="urn:hl7-org:v2xml" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:hl7-org:v2xml">
    <xsd:include schemaLocation="2_3_segments.xsd"/>
    <xsd:complexType name="RRD_O02.CONTENT">
        <xsd:sequence>
            <xsd:element ref="MSH"/>
            <xsd:element ref="MSA"/>
            <xsd:element minOccurs="0" ref="ERR"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
            <xsd:element minOccurs="0" ref="RRD_O02.PATIENT"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRD_O02" type="RRD_O02.CONTENT"/>
    <xsd:complexType name="RRD_O02.PATIENT.CONTENT">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="RRD_O02.RESPONSE"/>
            <xsd:element maxOccurs="unbounded" ref="RRD_O02.ORDER"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRD_O02.PATIENT" type="RRD_O02.PATIENT.CONTENT"/>
    <xsd:complexType name="RRD_O02.RESPONSE.CONTENT">
        <xsd:sequence>
            <xsd:element ref="PID"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRD_O02.RESPONSE" type="RRD_O02.RESPONSE.CONTENT"/>
    <xsd:complexType name="RRD_O02.ORDER.CONTENT">
        <xsd:sequence>
            <xsd:element ref="ORC"/>
            <xsd:element minOccurs="0" ref="RRD_O02.DISPENSE"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRD_O02.ORDER" type="RRD_O02.ORDER.CONTENT"/>
    <xsd:complexType name="RRD_O02.DISPENSE.CONTENT">
        <xsd:sequence>
            <xsd:element ref="RXD"/>
            <xsd:element maxOccurs="unbounded" ref="RXR"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="RXC"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRD_O02.DISPENSE" type="RRD_O02.DISPENSE.CONTENT"/>
</xsd:schema>
