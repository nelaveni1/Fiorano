            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
            <xsd:element minOccurs="0" ref="CSU_C12.VISIT"/>
            <xsd:element ref="CSR"/>
            <xsd:element maxOccurs="unbounded" ref="CSU_C12.STUDY_PHASE"/>
