    <xsd:complexType name="CSU_C12.RX_ADMIN.CONTENT">
        <xsd:sequence>
            <xsd:element ref="RXA"/>
            <xsd:element ref="RXR"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="CSU_C12.RX_ADMIN" type="CSU_C12.RX_ADMIN.CONTENT"/>
