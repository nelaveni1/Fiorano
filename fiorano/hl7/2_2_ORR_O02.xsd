    <xsd:element name="ORR_O02.ORDER" type="ORR_O02.ORDER.CONTENT"/>
    <xsd:complexType name="ORR_O02.ORDER_DETAIL.CONTENT">
        <xsd:sequence>
            <xsd:element ref="OBR"/>
