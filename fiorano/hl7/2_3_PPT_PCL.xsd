        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="PPT_PCL.ORDER" type="PPT_PCL.ORDER.CONTENT"/>
    <xsd:complexType name="PPT_PCL.ORDER_DETAIL.CONTENT">
        <xsd:sequence>
            <xsd:element ref="OBR"/>
