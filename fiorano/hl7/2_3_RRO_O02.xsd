<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="urn:hl7-org:v2xml" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:hl7-org:v2xml">
    <xsd:include schemaLocation="2_3_segments.xsd"/>
    <xsd:complexType name="RRO_O02.CONTENT">
        <xsd:sequence>
            <xsd:element ref="MSH"/>
            <xsd:element ref="MSA"/>
            <xsd:element minOccurs="0" ref="ERR"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
            <xsd:element minOccurs="0" ref="RRO_O02.RESPONSE"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRO_O02" type="RRO_O02.CONTENT"/>
    <xsd:complexType name="RRO_O02.RESPONSE.CONTENT">
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="RRO_O02.PATIENT"/>
            <xsd:element maxOccurs="unbounded" ref="RRO_O02.ORDER"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRO_O02.RESPONSE" type="RRO_O02.RESPONSE.CONTENT"/>
    <xsd:complexType name="RRO_O02.PATIENT.CONTENT">
        <xsd:sequence>
            <xsd:element ref="PID"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRO_O02.PATIENT" type="RRO_O02.PATIENT.CONTENT"/>
    <xsd:complexType name="RRO_O02.ORDER.CONTENT">
        <xsd:sequence>
            <xsd:element ref="ORC"/>
            <xsd:element minOccurs="0" ref="RRO_O02.ORDER_DETAIL"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRO_O02.ORDER" type="RRO_O02.ORDER.CONTENT"/>
    <xsd:complexType name="RRO_O02.ORDER_DETAIL.CONTENT">
        <xsd:sequence>
            <xsd:element ref="RXO"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
            <xsd:element maxOccurs="unbounded" ref="RXR"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="RXC"/>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="NTE"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="RRO_O02.ORDER_DETAIL" type="RRO_O02.ORDER_DETAIL.CONTENT"/>
</xsd:schema>
